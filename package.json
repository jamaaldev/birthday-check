{
  "version": "2.0.4",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamaaldev/birthday-check.git"
  },
  "keywords": [
    "birthday-check",
    "birthday",
    "age",
    "day left",
    "day gone"
  ],
  "name": "birthday-check",
  "author": "jamaaldev",
  "bugs": {
    "url": "https://github.com/jamaaldev/birthday-check/issues"
  },
  "homepage": "https://github.com/jamaaldev/birthday-check#readme",

  "module": "dist/birthday.esm.js",
  "size-limit": [
    {
      "path": "dist/birthday.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/birthday.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "husky": "^7.0.4",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
